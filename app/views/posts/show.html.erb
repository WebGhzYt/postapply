<h1>Posted By you</h1>


<table bgcolor="yellow" align="center" width="100%">
<ul class="users">
<% if @posts.any? %>
<tr>
	<td>Post Id</td>
	<td>Post Title</td>
	<td>Posting Date</td>
	<td>Laste Update</td>

	
</tr>
  <% @posts.each do |post| %>
  <% if @current_user.id == post.user_id %>
			<tr>
	     		<td><%= link_to post.id, '#' %></td>
	     		<td><%= link_to post.title, '#' %></td>
	     	    <td><% link_to post.created_at, '#' %></td>
	     	    <td><% link_to post.updated_at, '#' %></td>

			</tr>
			<% end %>
  <% end %>
<% else %>
	No any Jobs
<% end %>

</ul>
</table>




     
     