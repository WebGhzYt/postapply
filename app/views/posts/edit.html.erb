<h1>Update your profile</h1>
<div class="row">
 
  <div class="col-md-6 col-md-offset-3">

    <%= form_for(@post) do |f| %>
    <%= render 'shared/error_messages' %>
      <%= f.label :title %>
      <%= f.text_field :title %>
      <%= f.label :Experience_Month %>
      <%= f.text_field :experience %>
      <%= f.label :salary %>
      <%= f.text_field :salary %>
      <%= f.label :post %>
      <%= f.text_field :post %>
      <%= f.label :note %>
      <%= f.text_area :note %>




      <%= f.submit "Update", class: "btn btn-primary" %>
    <% end %>
  </div>

</div>
<center><%= link_to "Jobs Posted By me ", post_path(@current_user), class: "btn btn-lg btn-primary" %></center>