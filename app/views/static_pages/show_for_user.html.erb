   	<% if @temp == 3 && apply.post_id == post.id && current_user.id == apply.user_id  %>
   	Not applyed
   	
   		<% @temp = 2%>
   		<% end %>
   	<% break %>
   	<% end %>

   <% end %>

  <%end %>
<% end %>